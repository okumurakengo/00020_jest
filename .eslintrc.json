{
    "env": {
        "es6": true,
        "node": true,
        "jest": true
    },
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module"
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:prettier/recommended"
    ],
    "rules": {
        "prettier/prettier": ["error"],
        "no-console": "off",
        "no-unused-vars": "off",
        "react/prop-types": "off"
    }
}
